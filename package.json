{
  "name": "textTrends",
  "description": "Analyze word usage trends throughout a text",
  "main": "src/textTrendsIndex.html",
  "dependencies": {
    "jquery": "^3.0.0",
    "lodash": "^4",
    "react": "^15",
    "react-addons-css-transition-group": "^15.1.0",
    "react-d3": "^0.4.0",
    "react-d3-zoom": "^1.1.2",
    "react-dom": "^15",
    "react-typeahead": "^2.0.0-alpha.5",
    "tinycolor": "0.0.1",
    "tinycolor2": "^1.3.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "catw": "^1.0.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "less": "^2.5.3",
    "less-watch-compiler": "^1.2.3",
    "lessify": "^1.0.1",
    "watchify": "^3"
  },
  "scripts": {
    "watch": "npm run watch-js & npm run watch-css",
    "watch-css": "less-watch-compiler style static textTrends.less",
    "watch-js": "watchify -d -v -t [ babelify --presets [ es2015 react ] ] js/main.js -o static/bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      [
        "lessify"
      ]
    ]
  }
}
